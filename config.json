{
  "server": {
    "listen": ":8443",
    "dial": [
      "site1.example.com:8443",
      "site2.example.com:8443"
    ],
    "cert": "server-cert.pem",
    "key": "server-key.pem",
    "authcerts": [
      "client1-cert.pem",
      "client2-cert.pem"
    ]
  },
  "local": {
    "listen": "127.0.0.1:8080",
    "forward": "127.0.0.1:80"
  }
}