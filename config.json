{
  "server": [
    {
      "listen": "0.0.0.0:12345"
    }
  ],
  "client": [
    {
      "listen": "127.0.0.1:8119",
      "dial": "peer2.example.com:12345",
      "proxy": [
        {
          "listen": ":5201",
          "forward": "gateway.peer2.lan:5201"
        }
      ]
    }
  ],
  "cert": "peer1-cert.pem",
  "key": "peer1-key.pem",
  "authcerts": [
    "peer2-cert.pem"
  ]
}