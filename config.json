{
  "server": [
    {
      "listen": "0.0.0.0:12345",
      "forward": "127.0.0.1:8080"
    }
  ],
  "client": [
    {
      "listen": "127.0.0.1:8080",
      "dial": "example.com:12345"
    }
  ],
  "cert": "server-cert.pem",
  "key": "server-key.pem",
  "authcerts": [
    "client1-cert.pem",
    "client2-cert.pem"
  ]
}